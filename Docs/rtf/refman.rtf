00029         $sensor_id = $row[{\cf22 'id'}];\par
00030     \} {\cf19 else} \{\par
00031         {\cf20 // 2. Si no existe, lo creamos}\par
00032         $sqlInsertarSensor = {\cf22 "INSERT INTO sensor (mac, problema) VALUES (?, 0)"};\par
00033         $stmtInsertar = $conn->prepare($sqlInsertarSensor);\par
00034         $stmtInsertar->bind_param({\cf22 "s"}, $mac);\par
00035 \par
00036         {\cf19 if} ($stmtInsertar->execute()) \{\par
00037             $sensor_id = $conn->insert_id;\par
00038         \} {\cf19 else} \{\par
00039             {\cf19 return} [{\cf22 "status"} => {\cf22 "error"}, {\cf22 "mensaje"} => {\cf22 "Error al crear el sensor: "} . $conn->error];\par
00040         \}\par
00041     \}\par
00042 \par
00043     {\cf20 // 3. Cerrar relaciones anteriores activas de ese sensor}\par
00044     $sqlCerrarRelaciones = {\cf22 "UPDATE usuario_sensor }\par
00045 {\cf22                             SET actual = 0, fin_relacion = NOW() }\par
00046 {\cf22                             WHERE sensor_id = ? AND actual = 1"};\par
00047     $stmtCerrar = $conn->prepare($sqlCerrarRelaciones);\par
00048     $stmtCerrar->bind_param({\cf22 "i"}, $sensor_id);\par
00049     $stmtCerrar->execute();\par
