00037             {\cf22 "filas_relaciones_finalizadas"} => $stmtFin->affected_rows\par
00038         ];\par
00039     \} {\cf19 else} \{\par
00040         {\cf19 return} [{\cf22 "status"} => {\cf22 "error"}, {\cf22 "mensaje"} => {\cf22 "Error al actualizar sensor: "} . $conn->error];\par
00041     \}\par
00042 \}\par
}
