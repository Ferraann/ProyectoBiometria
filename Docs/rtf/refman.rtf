00011 \par
00012 {\cf17 function} marcarSensorConProblemas($conn, $data)\par
00013 \{\par
00014     {\cf19 if} (!isset($data[{\cf22 'sensor_id'}])) \{\par
00015         {\cf19 return} [{\cf22 "status"} => {\cf22 "error"}, {\cf22 "mensaje"} => {\cf22 "Falta el par\'E1metro sensor_id."}];\par
