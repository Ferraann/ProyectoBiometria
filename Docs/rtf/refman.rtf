00022 {\cf20 //------------------------------------------------------------------}\par
00023 {\cf20 // Clase LogicaNegocio}\par
00024 {\cf20 //}\par
00025 {\cf20 // Descripci\'F3n:}\par
00026 {\cf20 //  Esta clase se encargar de hacer toda la logica de negocio de}\par
00027 {\cf20 //  la app movil al servidor, se declara metodos post,get,insert...}\par
00028 {\cf20 //------------------------------------------------------------------}\par
00029 {\cf17 public} {\cf17 class }LogicaNegocio \{\par
00030     {\cf20 //-------------------------------------------------------------------------------------------}\par
00031     {\cf20 //     Nombre:txt, Apellidos:txt, email:txt, contrase\'F1a:txt --> postRegistro()}\par
00032     {\cf20 //-------------------------------------------------------------------------------------------}\par
00033     {\cf17 public} {\cf17 static} {\cf18 void} PostRegistro(String Nombre, String Apellidos, String Correo, String Contrasenya, Context contexto) \{\par
00034 \par
00035         ApiService api = ApiCliente.getApiService();\par
00036 \par
00037         PojoUsuario usuario = {\cf17 new} PojoUsuario();\par
00038         usuario.setNombre(Nombre);\par
00039         usuario.setApellidos(Apellidos);\par
00040         usuario.setCorreo(Correo);\par
00041         usuario.setContrasenya(Contrasenya);\par
00042         usuario.setAction({\cf22 "registrarUsuario"});\par
00043 \par
00044         Call<PojoRespuestaServidor> call = api.datosRegistro(usuario);\par
00045 \par
00046         {\cf20 //Ejecutamos la llamada post de forma asincrona, con un callback.Lo primero que hacemos es cojer la respuesta}\par
00047         {\cf20 //del servido, al recibirlo comparamos si ha fallado algo y si la respuesta en si tiene cuerpo. Si no se cumple}\par
00048         {\cf20 //ninguna de est\'E1s dos c\'F3ndiciones significa que algo a ocurriod en la conexi\'F3n. Si por el contrario es favorable}\par
00049         {\cf20 //la respuesta lo metemos en una clase pojo para poder usarlo de forma facil. Si la respuesta es aceptada, en este}\par
00050         {\cf20 //caso significa que la cuenta de la persona no est\'E1 creado teniendo en cuenta su email y si no es as\'ED es lo contrario}\par
00051         call.enqueue({\cf17 new} Callback<PojoRespuestaServidor>() \{\par
00052             @Override\par
00053             {\cf17 public} {\cf18 void} onResponse(Call<PojoRespuestaServidor> call, Response<PojoRespuestaServidor> response) \{\par
00054 \par
00055                 {\cf19 if}(!response.isSuccessful()||response.body()=={\cf17 null})\{\par
00056                     Log.d({\cf22 "Login"}, {\cf22 "Error en la respuesta: "} + response.code());\par
00057                     {\cf19 return};\par
00058                 \}\par
00059 \par
00060                 PojoRespuestaServidor respuesta = response.body();\par
00061 \par
00062                 {\cf20 // Miramos el status que viene del servidor}\par
00063                 {\cf19 if} (!{\cf22 "ok"}.equalsIgnoreCase(respuesta.getStatus())) \{\par
00064                     Log.w({\cf22 "API"}, {\cf22 "No funciona: "} + respuesta.getMensaje());\par
00065                     Toast.makeText(contexto, {\cf22 "Mail ya registrado"}, Toast.LENGTH_SHORT).show();\par
00066                     {\cf19 return};\par
00067                 \}\par
00068 \par
00069 \par
00070 \par
00071                 Toast.makeText(contexto,  respuesta.getMensaje() , Toast.LENGTH_SHORT).show();\par
00072 \par
00073 {\cf20 //                SharedPreferences prefs = contexto.getSharedPreferences("SesionUsuario", Context.MODE_PRIVATE);}\par
00074 {\cf20 //                prefs.edit()}\par
00075 {\cf20 //                        .putString("id", usuarioServidor.getId())}\par
00076 {\cf20 //                        .putString("nombre", usuarioServidor.getNombre())}\par
00077 {\cf20 //                        .putString("apellidos", usuarioServidor.getApellidos())}\par
00078 {\cf20 //                        // adapta "getCorreo" o "getGmail" seg\'FAn lo que tengas}\par
00079 {\cf20 //                        .putString("correo", usuarioServidor.getCorreo())}\par
00080 {\cf20 //                        .apply();}\par
00081 \par
00082             \}\par
00083 \par
00084             @Override\par
00085             {\cf17 public} {\cf18 void} onFailure(Call<PojoRespuestaServidor> call, Throwable t) \{\par
00086                 Log.e({\cf22 "API"}, {\cf22 "Error de conexi\'F3n: "} + t.getMessage());\par
00087             \}\par
00088         \});\par
00089 \par
00090     \}\par
00091 \par
00092 \par
00093     {\cf20 //-------------------------------------------------------------------------------------------}\par
00094     {\cf20 //     Email:txt, Contrase\'F1a:txt, Contexto:context --> postRegistro()}\par
00095     {\cf20 //-------------------------------------------------------------------------------------------}\par
00096     {\cf17 public} {\cf17 static} {\cf18 void} PostLogin(String correo, String contrasenya, Context contexto)\{\par
00097         ApiService apiService = ApiCliente.getApiService();\par
00098 \par
00099 \par
00100         PojoUsuario usuario = {\cf17 new} PojoUsuario();\par
00101         usuario.setCorreo(correo);\par
00102         usuario.setContrasenya(contrasenya);\par
00103         usuario.setAction({\cf22 "login"});\par
00104 \par
00105 \par
00106         Call<PojoRespuestaServidor> call = apiService.loginUsuario(usuario);\par
00107 \par
00108 \par
00109         call.enqueue({\cf17 new} Callback<PojoRespuestaServidor>() \{\par
00110             @Override\par
00111             {\cf17 public} {\cf18 void} onResponse(Call<PojoRespuestaServidor> call, Response<PojoRespuestaServidor> response) \{\par
00112                 {\cf19 if}(!response.isSuccessful()||response.body()=={\cf17 null})\{\par
00113                     Log.d({\cf22 "Login"}, {\cf22 "Error en la respuesta: "} + response.code());\par
00114                     {\cf19 return};\par
00115 \par
00116                 \}\par
00117                 PojoRespuestaServidor respuesta = response.body();\par
00118 \par
00119                 {\cf20 // Miramos el status que viene del servidor}\par
00120                 {\cf19 if} (!{\cf22 "ok"}.equalsIgnoreCase(respuesta.getStatus())) \{\par
00121                     {\cf20 // Si viene un mensaje de error, lo mostramos en log (o Toast)}\par
00122                     Log.d({\cf22 "Login"}, {\cf22 "Login fallido: "} + respuesta.getMensaje());\par
00123                     {\cf19 return};\par
00124                 \}\par
00125                 PojoUsuario usuarioServidor = respuesta.getUsuario();\par
00126                 {\cf20 // 4.4 Guardamos los datos del usuario en SharedPreferences}\par
00127                 SharedPreferences prefs = contexto.getSharedPreferences({\cf22 "SesionUsuario"}, Context.MODE_PRIVATE);\par
00128                 prefs.edit()\par
00129                         .putString({\cf22 "id"}, usuarioServidor.getId())\par
00130                         .putString({\cf22 "nombre"}, usuarioServidor.getNombre())\par
00131                         .putString({\cf22 "apellidos"}, usuarioServidor.getApellidos())\par
00132                         .putString({\cf22 "correo"}, usuarioServidor.getCorreo())\par
00133                         .apply();\par
00134 \par
00135                 Intent intent = {\cf17 new} Intent(contexto, HomeActivity.class);\par
00136                 contexto.startActivity(intent);\par
00137             \}\par
00138 \par
00139             @Override\par
00140             {\cf17 public} {\cf18 void} onFailure(Call<PojoRespuestaServidor> call, Throwable t) \{\par
00141                 Log.e({\cf22 "Login"}, {\cf22 "Error en conexi\'F3n: "} + t.getMessage());\par
00142             \}\par
00143         \});\par
00144     \}\par
00145 \par
00146     {\cf20 //--------------------------------------------------------------------------------}\par
00147     {\cf20 //  Nombre: txt, Apellidos: txt, Email: txt,}\par
00148     {\cf20 //--------------------------------------------------------------------------------}\par
00149     {\cf17 public} {\cf17 static} {\cf18 void} putModificarDatos(PojoUsuario usuario,Context contexto) \{\par
00150         ApiService apiService = ApiCliente.getApiService(); {\cf20 // Usamos tu ApiCliente existente}\par
00151         Call<PojoRespuestaServidor> call = apiService.modificarDatos(usuario);\par
00152 \par
00153         call.enqueue({\cf17 new} Callback<PojoRespuestaServidor>() \{\par
00154             @Override\par
00155             {\cf17 public} {\cf18 void} onResponse(Call<PojoRespuestaServidor> call, Response<PojoRespuestaServidor> response) \{\par
00156                 {\cf19 if} (!response.isSuccessful()) \{\par
00157                     Log.d({\cf22 "Modifcaci\'F3n"}, {\cf22 "Error al modificar datos"});\par
00158                     {\cf19 return};\par
00159                 \}\par
00160 \par
00161                 PojoRespuestaServidor respuesta = response.body();\par
00162                 PojoUsuario usuarioServidor = respuesta.getUsuario();\par
