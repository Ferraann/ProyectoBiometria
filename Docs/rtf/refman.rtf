00022     $sqlBuscarSensor = {\cf22 "SELECT id FROM sensor WHERE mac = ?"};\par
00023     $stmtBuscar = $conn->prepare($sqlBuscarSensor);\par
00024     $stmtBuscar->bind_param({\cf22 "s"}, $mac);\par
00025     $stmtBuscar->execute();\par
00026     $result = $stmtBuscar->get_result();\par
00027 \par
