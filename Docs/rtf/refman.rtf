00031     \}\par
00032 \par
00033     {\cf20 // 3. Activar usuario}\par
00034     $sql2 = {\cf22 "UPDATE usuario SET activo = 1, token = NULL, token_expira = NULL WHERE id = ?"};\par
00035     $stmt2 = $conn->prepare($sql2);\par
