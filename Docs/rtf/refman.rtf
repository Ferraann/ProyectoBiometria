00149             $mailer->Port = $config[{\cf22 'port'}];\par
00150         \} elseif ($isSMTPS) \{\par
00151             $mailer->Port = SMTP::DEFAULT_SECURE_PORT;\par
00152         \}\par
00153 \par
00154         $mailer->SMTPAuth = isset($config[{\cf22 'user'}]) || isset($config[{\cf22 'pass'}]);\par
00155 \par
00156         {\cf19 if} (isset($config[{\cf22 'user'}])) \{\par
00157             $mailer->Username = $config[{\cf22 'user'}];\par
00158         \}\par
00159 \par
00160         {\cf19 if} (isset($config[{\cf22 'pass'}])) \{\par
00161             $mailer->Password = $config[{\cf22 'pass'}];\par
00162         \}\par
00163     \}\par
00164 \par
00173     {\cf17 private} {\cf17 function} configureOptions(PHPMailer $mailer, $options)\par
00174     \{\par
00175         $allowedOptions = get_object_vars($mailer);\par
00176 \par
00177         unset($allowedOptions[{\cf22 'Mailer'}]);\par
00178         unset($allowedOptions[{\cf22 'SMTPAuth'}]);\par
00179         unset($allowedOptions[{\cf22 'Username'}]);\par
00180         unset($allowedOptions[{\cf22 'Password'}]);\par
