00003 {\cf17 import} java.math.BigInteger;\par
00004 {\cf17 import} java.nio.ByteBuffer;\par
00005 {\cf17 import} java.util.UUID;\par
00006 \par
00007 {\cf20 // -----------------------------------------------------------------------------------}\par
00008 {\cf20 // @author: Jordi Bataller i Mascarell}\par
00009 {\cf20 // -----------------------------------------------------------------------------------}\par
00010 {\cf17 public} {\cf17 class }Utilidades \{\par
00011     {\cf20 // -------------------------------------------------------------------------------}\par
00012     {\cf20 // Dado un string te devuelve los bytes}\par
00013     {\cf20 // -------------------------------------------------------------------------------}\par
00014     {\cf17 public} {\cf17 static} {\cf18 byte}[] stringToBytes ( String texto ) \{\par
00015         {\cf19 return} texto.getBytes();\par
00016         {\cf20 // byte[] b = string.getBytes(StandardCharsets.UTF_8); // Ja}\par
00017     \} {\cf20 // ()}\par
00018 \par
00019     {\cf20 // -------------------------------------------------------------------------------}\par
00020     {\cf20 // Dado el string te devuelve el uuid. Si el tama\'F1o no es 16 da un error.}\par
00021     {\cf20 // -------------------------------------------------------------------------------}\par
00022     {\cf17 public} {\cf17 static} UUID stringToUUID( String uuid ) \{\par
00023         {\cf19 if} ( uuid.length() != 16 ) \{\par
00024             {\cf19 throw} {\cf17 new} Error( {\cf22 "stringUUID: string no tiene 16 caracteres "});\par
00025         \}\par
00026         {\cf18 byte}[] comoBytes = uuid.getBytes();\par
00027 \par
00028         String masSignificativo = uuid.substring(0, 8);\par
00029         String menosSignificativo = uuid.substring(8, 16);\par
00030         UUID res = {\cf17 new} UUID( Utilidades.bytesToLong( masSignificativo.getBytes() ), Utilidades.bytesToLong( menosSignificativo.getBytes() ) );\par
00031 \par
00032         {\cf20 // Log.d( MainActivity.ETIQUETA_LOG, " \\n\\n***** stringToUUID *** " + uuid  + "=?=" + Utilidades.uuidToString( res ) );}\par
00033 \par
00034         {\cf20 // UUID res = UUID.nameUUIDFromBytes( comoBytes ); no va como quiero}\par
00035 \par
00036         {\cf19 return} res;\par
00037     \} {\cf20 // ()}\par
00038 \par
00039     {\cf20 // -------------------------------------------------------------------------------}\par
00040     {\cf20 // Dado un UUID lo transforma a string y lo devuelve}\par
00041     {\cf20 // -------------------------------------------------------------------------------}\par
00042     {\cf17 public} {\cf17 static} String uuidToString ( UUID uuid ) \{\par
00043         {\cf19 return} bytesToString( dosLongToBytes( uuid.getMostSignificantBits(), uuid.getLeastSignificantBits() ) );\par
00044     \} {\cf20 // ()}\par
00045 \par
00046     {\cf20 // -------------------------------------------------------------------------------}\par
00047     {\cf20 // Dado el uuid lo transforma a hexadecimal}\par
00048     {\cf20 // -------------------------------------------------------------------------------}\par
00049     {\cf17 public} {\cf17 static} String uuidToHexString ( UUID uuid ) \{\par
00050         {\cf19 return} bytesToHexString( dosLongToBytes( uuid.getMostSignificantBits(), uuid.getLeastSignificantBits() ) );\par
00051     \} {\cf20 // ()}\par
00052 \par
00053     {\cf20 // -------------------------------------------------------------------------------}\par
00054     {\cf20 // Dados los bytes devuelve un string con los bytes.}\par
00055     {\cf20 // -------------------------------------------------------------------------------}\par
00056     {\cf17 public} {\cf17 static} String bytesToString( {\cf18 byte}[] bytes ) \{\par
00057         {\cf19 if} (bytes == {\cf17 null} ) \{\par
00058             {\cf19 return} {\cf22 ""};\par
00059         \}\par
00060 \par
00061         StringBuilder sb = {\cf17 new} StringBuilder();\par
00062         {\cf19 for} ({\cf18 byte} b : bytes) \{\par
00063             sb.append( ({\cf18 char}) b );\par
00064         \}\par
00065         {\cf19 return} sb.toString();\par
00066     \}\par
00067 \par
00068     {\cf20 // -------------------------------------------------------------------------------}\par
00069     {\cf20 // Dados dos long devuelve un buffer en forma de array.}\par
00070     {\cf20 // -------------------------------------------------------------------------------}\par
00071     {\cf17 public} {\cf17 static} {\cf18 byte}[] dosLongToBytes( {\cf18 long} masSignificativos, {\cf18 long} menosSignificativos ) \{\par
00072         ByteBuffer buffer = ByteBuffer.allocate( 2 * Long.BYTES );\par
00073         buffer.putLong( masSignificativos );\par
00074         buffer.putLong( menosSignificativos );\par
00075         {\cf19 return} buffer.array();\par
00076     \}\par
00077 \par
00078     {\cf20 // -------------------------------------------------------------------------------}\par
00079     {\cf20 // Conversor de un array de bytes a un entero int}\par
00080     {\cf20 // -------------------------------------------------------------------------------}\par
00081     {\cf17 public} {\cf17 static} {\cf18 int} bytesToInt( {\cf18 byte}[] bytes ) \{\par
00082         {\cf19 return} {\cf17 new} BigInteger(bytes).intValue();\par
00083     \}\par
00084 \par
00085     {\cf20 // -------------------------------------------------------------------------------}\par
00086     {\cf20 // Conversor de un array de bytes a un long}\par
00087     {\cf20 // -------------------------------------------------------------------------------}\par
00088     {\cf17 public} {\cf17 static} {\cf18 long} bytesToLong( {\cf18 byte}[] bytes ) \{\par
00089         {\cf19 return} {\cf17 new} BigInteger(bytes).longValue();\par
00090     \}\par
00091 \par
00092     {\cf20 // -------------------------------------------------------------------------------}\par
00093     {\cf20 // -------------------------------------------------------------------------------}\par
00094     {\cf17 public} {\cf17 static} {\cf18 int} bytesToIntOK( {\cf18 byte}[] bytes ) \{\par
00095         {\cf19 if} (bytes == {\cf17 null} ) \{\par
00096             {\cf19 return} 0;\par
00097         \}\par
00098 \par
00099         {\cf19 if} ( bytes.length > 4 ) \{\par
00100             {\cf19 throw} {\cf17 new} Error( {\cf22 "demasiados bytes para pasar a int "});\par
00101         \}\par
00102         {\cf18 int} res = 0;\par
00103 \par
00104 \par
00105 \par
00106         {\cf19 for}( {\cf18 byte} b : bytes ) \{\par
00107            {\cf20 /*}\par
00108 {\cf20            Log.d( MainActivity.ETIQUETA_LOG, "bytesToInt(): byte: hex=" + Integer.toHexString( b )}\par
00109 {\cf20                    + " dec=" + b + " bin=" + Integer.toBinaryString( b ) +}\par
00110 {\cf20                    " hex=" + Byte.toString( b )}\par
00111 {\cf20            );}\par
00112 {\cf20            */}\par
00113             res =  (res << 8) {\cf20 // * 16}\par
00114                     + (b & 0xFF); {\cf20 // para quedarse con 1 byte (2 cuartetos) de lo que haya en b}\par
00115         \} {\cf20 // for}\par
00116 \par
00117         {\cf19 if} ( (bytes[ 0 ] & 0x8) != 0 ) \{\par
00118             {\cf20 // si tiene signo negativo (un 1 a la izquierda del primer byte}\par
00119             res = -(~(byte)res)-1; {\cf20 // complemento a 2 (~) de res pero como byte, -1}\par
00120         \}\par
00121        {\cf20 /*}\par
00122 {\cf20         Log.d( MainActivity.ETIQUETA_LOG, "bytesToInt(): res = " + res + " ~res=" + (res ^ 0xffff)}\par
00123 {\cf20                 + "~res=" + ~((byte) res)}\par
00124 {\cf20         );}\par
00125 {\cf20         */}\par
00126 \par
00127         {\cf19 return} res;\par
00128     \} {\cf20 // ()}\par
00129 \par
00130     {\cf20 // -------------------------------------------------------------------------------}\par
00131     {\cf20 // Dados los bytes si no son nulos los transforma a hexadecimal y los devuelve en forma de string}\par
00132     {\cf20 // -------------------------------------------------------------------------------}\par
00133     {\cf17 public} {\cf17 static} String bytesToHexString( {\cf18 byte}[] bytes ) \{\par
00134 \par
00135         {\cf19 if} (bytes == {\cf17 null} ) \{\par
00136             {\cf19 return} {\cf22 ""};\par
00137         \}\par
00138 \par
00139         StringBuilder sb = {\cf17 new} StringBuilder();\par
00140         {\cf19 for} ({\cf18 byte} b : bytes) \{\par
00141             sb.append(String.format({\cf22 "%02x"}, b));\par
00142             sb.append({\cf23 ':'});\par
00143         \}\par
00144         {\cf19 return} sb.toString();\par
00145     \} {\cf20 // ()}\par
00146 \} {\cf20 // class}\par
00147 {\cf20 // -----------------------------------------------------------------------------------}\par
00148 {\cf20 // -----------------------------------------------------------------------------------}\par
00149 {\cf20 // -----------------------------------------------------------------------------------}\par
00150 {\cf20 // -----------------------------------------------------------------------------------}\par
}
