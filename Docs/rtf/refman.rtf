00016 {\cf20 // FUNCI\'D3N 1X: Obtener estad\'EDsticas generales: n\'BA de sensores,n\'BA de sensores activos, valor promedio, \'FAltima medici\'F3n}\par
00017 {\cf20 // -------------------------------------------------------------}\par
00018 {\cf17 function} obtenerEstadisticas($conn)\par
00019 \{\par
00020     $stats = [];\par
00021 \par
00022     $result = $conn->query({\cf22 "SELECT COUNT(*) AS total FROM sensor"});\par
00023     $stats[{\cf22 'total_sensores'}] = $result->fetch_assoc()[{\cf22 'total'}];\par
00024 \par
00025     $result = $conn->query({\cf22 "SELECT COUNT(*) AS activos FROM usuario_sensor WHERE actual = 1"});\par
00026     $stats[{\cf22 'sensores_activos'}] = $result->fetch_assoc()[{\cf22 'activos'}];\par
00027 \par
00028     $result = $conn->query({\cf22 "SELECT MAX(hora) AS ultima_medicion FROM medicion"});\par
00029     $stats[{\cf22 'ultima_medicion'}] = $result->fetch_assoc()[{\cf22 'ultima_medicion'}];\par
00030 \par
00031     {\cf19 return} [{\cf22 "status"} => {\cf22 "ok"}, {\cf22 "estadisticas"} => $stats];\par
00032 \}\par
00033 \par
00034 {\cf20 // -------------------------------------------------------------}\par
00035 {\cf20 // FUNCI\'D3N 1X: Obtener promedio de cada tipo de mediciones en un rango geogr\'E1fico}\par
00036 {\cf20 // Puede que no funcione todavia}\par
00037 {\cf20 // -------------------------------------------------------------}\par
00038 {\cf17 function} promedioPorRango($conn, $lat_min, $lat_max, $lon_min, $lon_max)\par
00039 \{\par
00040     $sql = {\cf22 "}\par
00041 {\cf22         SELECT tm.medida, tm.unidad, AVG(m.valor) AS promedio}\par
00042 {\cf22         FROM medicion m}\par
00043 {\cf22         INNER JOIN tipo_medicion tm ON m.tipo_medicion_id = tm.id}\par
00044 {\cf22         WHERE }\par
00045 {\cf22             CAST(SUBSTRING_INDEX(m.localizacion, ',', 1) AS DECIMAL(10,6)) BETWEEN ? AND ?}\par
00046 {\cf22             AND CAST(SUBSTRING_INDEX(m.localizacion, ',', -1) AS DECIMAL(10,6)) BETWEEN ? AND ?}\par
00047 {\cf22         GROUP BY tm.id, tm.medida, tm.unidad}\par
00048 {\cf22         ORDER BY tm.medida ASC}\par
00049 {\cf22     "};\par
00050 \par
00051     $stmt = $conn->prepare($sql);\par
00052     $stmt->bind_param({\cf22 "dddd"}, $lat_min, $lat_max, $lon_min, $lon_max);\par
00053     $stmt->execute();\par
00054     $result = $stmt->get_result();\par
00055 \par
00056     $promedios = [];\par
00057     {\cf19 while} ($row = $result->fetch_assoc()) \{\par
00058         $promedios[] = [\par
00059             {\cf22 "medida"} => $row[{\cf22 'medida'}],\par
00060             {\cf22 "unidad"} => $row[{\cf22 'unidad'}],\par
00061             {\cf22 "promedio"} => round($row[{\cf22 'promedio'}], 2)\par
00062         ];\par
00063     \}\par
00064 \par
00065     {\cf19 return} [\par
00066         {\cf22 "status"} => {\cf22 "ok"},\par
00067         {\cf22 "rango"} => [\par
00068             {\cf22 "lat_min"} => $lat_min,\par
00069             {\cf22 "lat_max"} => $lat_max,\par
00070             {\cf22 "lon_min"} => $lon_min,\par
00071             {\cf22 "lon_max"} => $lon_max\par
00072         ],\par
00073         {\cf22 "promedios"} => $promedios\par
00074     ];\par
00075 \}\par
00076 \par
00077 {\cf20 // -------------------------------------------------------------}\par
00078 {\cf20 // FUNCI\'D3N 15: Obtener promedio de cada tipo de mediciones en un rango geogr\'E1fico}\par
00079 {\cf20 // Puede que no funcione todavia}\par
00080 {\cf20 // -------------------------------------------------------------}\par
00081 {\cf17 function} modificarDatos($conn, $data)\{\par
00082     {\cf19 if} (!isset($data[{\cf22 'id'}])) \{\par
00083         {\cf19 return} [{\cf22 "status"} => {\cf22 "error"}, {\cf22 "mensaje"} => {\cf22 "Falta el ID del usuario"}];\par
00084     \}\par
00085 \par
00086     $id = $data[{\cf22 'id'}];\par
00087 \par
00088     {\cf20 // Obtenemos los valores enviados (si no vienen, no se modifican)}\par
00089     $nombre    = $data[{\cf22 'nombre'}]    ?? {\cf17 null};\par
00090     $apellidos = $data[{\cf22 'apellidos'}] ?? {\cf17 null};\par
00091     $correo    = $data[{\cf22 'gmail'}]    ?? {\cf17 null};\par
00092     $password  = $data[{\cf22 'password'}]  ?? {\cf17 null};\par
00093 \par
00094     {\cf20 // Construimos SQL din\'E1mico seg\'FAn lo que lleg\'F3}\par
00095     $campos = [];\par
00096     $params = [];\par
00097     $tipos  = {\cf22 ""};\par
00098 \par
00099     {\cf19 if} ($nombre !== {\cf17 null}) \{\par
00100         $campos[] = {\cf22 "nombre = ?"};\par
00101         $params[] = $nombre;\par
00102         $tipos   .= {\cf22 "s"};\par
00103     \}\par
00104 \par
00105     {\cf19 if} ($apellidos !== {\cf17 null}) \{\par
00106         $campos[] = {\cf22 "apellidos = ?"};\par
00107         $params[] = $apellidos;\par
00108         $tipos   .= {\cf22 "s"};\par
00109     \}\par
00110 \par
00111     {\cf19 if} ($correo !== {\cf17 null}) \{\par
00112         $campos[] = {\cf22 "gmail = ?"};\par
00113         $params[] = $correo;\par
00114         $tipos   .= {\cf22 "s"};\par
00115     \}\par
00116 \par
00117     {\cf19 if} ($password !== {\cf17 null}) \{\par
00118         $hash = password_hash($password, PASSWORD_DEFAULT);\par
00119         $campos[] = {\cf22 "password = ?"};\par
00120         $params[] = $hash;\par
00121         $tipos   .= {\cf22 "s"};\par
00122     \}\par
00123 \par
00124     {\cf19 if} (empty($campos)) \{\par
00125         {\cf19 return} [{\cf22 "status"} => {\cf22 "error"}, {\cf22 "mensaje"} => {\cf22 "No hay datos para actualizar"}];\par
00126     \}\par
00127 \par
00128     {\cf20 // SQL final}\par
00129     $sql = {\cf22 "UPDATE usuario SET "} . implode({\cf22 ", "}, $campos) . {\cf22 " WHERE id = ?"};\par
00130     $params[] = $id;\par
00131     $tipos   .= {\cf22 "i"};\par
00132 \par
00133     $stmt = $conn->prepare($sql);\par
00134     $stmt->bind_param($tipos, ...$params);\par
00135 \par
00136     {\cf19 if} (!$stmt->execute()) \{\par
00137         {\cf19 return} [{\cf22 "status"} => {\cf22 "error"}, {\cf22 "mensaje"} => $conn->error];\par
00138     \}\par
00139 \par
00140     {\cf20 // -------------------------------}\par
00141     {\cf20 //   OBTENER DATOS ACTUALIZADOS}\par
00142     {\cf20 // -------------------------------}\par
00143     $sqlUser = {\cf22 "SELECT id, nombre, apellidos, gmail AS correo FROM usuario WHERE id = ?"};\par
00144     $stmtUser = $conn->prepare($sqlUser);\par
00145     $stmtUser->bind_param({\cf22 "i"}, $id);\par
00146     $stmtUser->execute();\par
00147     $resUser = $stmtUser->get_result();\par
00148     $usuarioActualizado = $resUser->fetch_assoc();\par
00149 \par
00150     {\cf19 return} [\par
00151         {\cf22 "status"} => {\cf22 "ok"},\par
00152         {\cf22 "mensaje"} => {\cf22 "Usuario actualizado correctamente"},\par
00153         {\cf22 "usuario"} => $usuarioActualizado\par
00154     ];\par
00155 \}\par
00156 \par
00157 {\cf20 // -------------------------------------------------------------}\par
00158 {\cf20 // FUNCI\'D3N 11: Obtener incidencias activas}\par
00159 {\cf20 // -------------------------------------------------------------}\par
00160 {\cf17 function} obtenerIncidenciasActivas($conn)\par
00161 \{\par
00162     $sql = {\cf22 "}\par
00163 {\cf22     SELECT }\par
00164 {\cf22         i.id,}\par
00165 {\cf22         u.nombre                                   AS usuario,}\par
00166 {\cf22         i.titulo,}\par
00167 {\cf22         i.descripcion,}\par
00168 {\cf22         i.fecha_creacion,}\par
00169 {\cf22         e.nombre                                   AS estado,}\par
00170 {\cf22         i.id_tecnico,}\par
00171 {\cf22         COALESCE(tu.nombre, 'Sin asignar')         AS tecnico}\par
00172 {\cf22     FROM incidencias i}\par
00173 {\cf22     LEFT JOIN usuario u  ON i.id_user   = u.id}\par
00174 {\cf22     LEFT JOIN estado_incidencia e ON i.estado_id = e.id}\par
00175 {\cf22     LEFT JOIN tecnicos t          ON i.id_tecnico = t.usuario_id}\par
00176 {\cf22     LEFT JOIN usuario tu          ON t.usuario_id = tu.id}\par
00177 {\cf22     WHERE e.nombre NOT IN ('Cerrada', 'Cancelada')}\par
00178 {\cf22     ORDER BY i.fecha_creacion DESC}\par
00179 {\cf22 "};\par
00180 \par
00181     $result = $conn->query($sql);\par
00182     $incidencias = [];\par
00183     {\cf19 while} ($row = $result->fetch_assoc()) \{\par
00184         $incidencias[] = $row;\par
00185     \}\par
00186     {\cf19 return} $incidencias;\par
00187 \}\par
}
