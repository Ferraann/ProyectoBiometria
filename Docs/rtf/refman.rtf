01333 \par
01334             {\cf19 if} ($n === {\cf17 false}) \{\par
01335                 $message = $this->getError()[{\cf22 'detail'}];\par
01336 \par
01337                 $this->edebug(\par
01338                     {\cf22 'SMTP -> get_lines(): select failed ('} . $message . {\cf23 ')'},\par
01339                     self::DEBUG_LOWLEVEL\par
01340                 );\par
01341 \par
01342                 {\cf20 //stream_select returns false when the `select` system call is interrupted}\par
01343                 {\cf20 //by an incoming signal, try the select again}\par
01344                 {\cf19 if} (\par
01345                     stripos($message, {\cf22 'interrupted system call'}) !== {\cf17 false} ||\par
01346                     (\par
01347                         {\cf20 // on applications with a different locale than english, the message above is not found because}\par
01348                         {\cf20 // it's translated. So we also check for the SOCKET_EINTR constant which is defined under}\par
01349                         {\cf20 // Windows and UNIX-like platforms (if available on the platform).}\par
01350                         defined({\cf22 'SOCKET_EINTR'}) &&\par
01351                         stripos($message, {\cf22 'stream_select(): Unable to select ['} . SOCKET_EINTR . {\cf23 ']'}) !== {\cf17 false}\par
01352                     )\par
01353                 ) \{\par
01354                     $this->edebug(\par
01355                         {\cf22 'SMTP -> get_lines(): retrying stream_select'},\par
01356                         self::DEBUG_LOWLEVEL\par
01357                     );\par
01358                     $this->setError({\cf22 ''});\par
01359                     {\cf19 continue};\par
01360                 \}\par
01361 \par
01362                 {\cf19 break};\par
01363             \}\par
01364 \par
01365             {\cf19 if} (!$n) \{\par
01366                 $this->edebug(\par
01367                     {\cf22 'SMTP -> get_lines(): select timed-out in ('} . $this->Timelimit . {\cf22 ' sec)'},\par
01368                     self::DEBUG_LOWLEVEL\par
01369                 );\par
01370                 {\cf19 break};\par
01371             \}\par
01372 \par
01373             {\cf20 //Deliberate noise suppression - errors are handled afterwards}\par
01374             $str = @fgets($this->smtp_conn, self::MAX_REPLY_LENGTH);\par
01375             $this->edebug({\cf22 'SMTP INBOUND: "'} . trim($str) . {\cf23 '"'}, self::DEBUG_LOWLEVEL);\par
01376             $data .= $str;\par
01377             {\cf20 //If response is only 3 chars (not valid, but RFC5321 S4.2 says it must be handled),}\par
01378             {\cf20 //or 4th character is a space or a line break char, we are done reading, break the loop.}\par
01379             {\cf20 //String array access is a significant micro-optimisation over strlen}\par
01380             {\cf19 if} (!isset($str[3]) || $str[3] === {\cf23 ' '} || $str[3] === {\cf22 "\\r"} || $str[3] === {\cf22 "\\n"}) \{\par
01381                 {\cf19 break};\par
01382             \}\par
