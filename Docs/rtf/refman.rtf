00028         $base64 = preg_replace({\cf22 '/^data:image\\/\\w+;base64,/'}, {\cf22 ''}, $base64);\par
00029         $blob = base64_decode($base64, {\cf17 true});\par
00030         \par
00031         {\cf19 if} ($blob === {\cf17 false}) \{\par
00032             $stmt->close();\par
00033             {\cf19 return} [{\cf22 "status"} => {\cf22 "error"}, {\cf22 "mensaje"} => {\cf22 "Una de las im\'E1genes no es v\'E1lida."}];\par
00034         \}\par
00035 \par
00036         $stmt->bind_param({\cf22 "ib"}, $incidencia_id, $blob);\par
00037         $stmt->send_long_data(1, $blob);\par
00038         \par
00039         {\cf19 if} (!$stmt->execute()) \{\par
00040             $stmt->close();\par
00041             {\cf19 return} [{\cf22 "status"} => {\cf22 "error"}, {\cf22 "mensaje"} => {\cf22 "Error al guardar la foto."}];\par
00042         \}\par
00043     \}\par
00044 \par
00045     $stmt->close();\par
00046     {\cf19 return} [{\cf22 "status"} => {\cf22 "ok"}, {\cf22 "mensaje"} => {\cf22 "Fotos guardadas correctamente."}];\par
