00101     {\cf17 private} {\cf17 function} applyConfig(PHPMailer $mailer, $config)\par
00102     \{\par
00103         {\cf19 switch} ($config[{\cf22 'scheme'}]) \{\par
00104             {\cf19 case} {\cf22 'mail'}:\par
00105                 $mailer->isMail();\par
00106                 {\cf19 break};\par
00107             {\cf19 case} {\cf22 'sendmail'}:\par
00108                 $mailer->isSendmail();\par
00109                 {\cf19 break};\par
00110             {\cf19 case} {\cf22 'qmail'}:\par
00111                 $mailer->isQmail();\par
00112                 {\cf19 break};\par
00113             {\cf19 case} {\cf22 'smtp'}:\par
00114             {\cf19 case} {\cf22 'smtps'}:\par
00115                 $mailer->isSMTP();\par
00116                 $this->configureSMTP($mailer, $config);\par
00117                 {\cf19 break};\par
00118             {\cf19 default}:\par
00119                 {\cf19 throw} {\cf17 new} Exception(\par
00120                     sprintf(\par
00121                         {\cf22 'Invalid scheme: "%s". Allowed values: "mail", "sendmail", "qmail", "smtp", "smtps".'},\par
00122                         $config[{\cf22 'scheme'}]\par
00123                     )\par
00124                 );\par
00125         \}\par
00126 \par
00127         {\cf19 if} (isset($config[{\cf22 'query'}])) \{\par
