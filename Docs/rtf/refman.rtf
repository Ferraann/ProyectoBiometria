00007 {\cf20 // Descripci\'F3n:}\par
00008 {\cf20 // Funci\'F3n para asignar un sensor a un usuario, creando el sensor si }\par
00009 {\cf20 // no existe o modificar la relaci\'F3n si ya existe}\par
00010 {\cf20 // ------------------------------------------------------------------}\par
00011 \par
00012 {\cf17 function} crearSensorYRelacion($conn, $data)\par
00013 \{\par
00014     {\cf19 if} (!isset($data[{\cf22 'mac'}]) || !isset($data[{\cf22 'usuario_id'}])) \{\par
00015         {\cf19 return} [{\cf22 "status"} => {\cf22 "error"}, {\cf22 "mensaje"} => {\cf22 "Faltan par\'E1metros: mac y usuario_id son obligatorios."}];\par
