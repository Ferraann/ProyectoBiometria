05147                 {\cf19 continue};\par
05148             \}\par
05149             list($heading, $value) = explode({\cf23 ':'}, $line, 2);\par
05150             {\cf20 //Lower-case header name}\par
05151             $heading = strtolower($heading);\par
05152             {\cf20 //Collapse white space within the value, also convert WSP to space}\par
05153             $value = preg_replace({\cf22 '/[ \\t]+/'}, {\cf23 ' '}, $value);\par
05154             {\cf20 //RFC6376 is slightly unclear here - it says to delete space at the *end* of each value}\par
05155             {\cf20 //But then says to delete space before and after the colon.}\par
05156             {\cf20 //Net result is the same as trimming both ends of the value.}\par
05157             {\cf20 //By elimination, the same applies to the field name}\par
05158             $lines[$key] = trim($heading, {\cf22 " \\t"}) . {\cf23 ':'} . trim($value, {\cf22 " \\t"});\par
05159         \}\par
05160 \par
05161         {\cf19 return} implode(self::CRLF, $lines);\par
05162     \}\par
