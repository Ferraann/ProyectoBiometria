00195                 );\par
00196             \}\par
00197 \par
00198             {\cf19 switch} ($key) \{\par
00199                 {\cf19 case} {\cf22 'AllowEmpty'}:\par
00200                 {\cf19 case} {\cf22 'SMTPAutoTLS'}:\par
00201                 {\cf19 case} {\cf22 'SMTPKeepAlive'}:\par
00202                 {\cf19 case} {\cf22 'SingleTo'}:\par
00203                 {\cf19 case} {\cf22 'UseSendmailOptions'}:\par
00204                 {\cf19 case} {\cf22 'do_verp'}:\par
00205                 {\cf19 case} {\cf22 'DKIM_copyHeaderFields'}:\par
00206                     $mailer->$key = (bool) $value;\par
00207                     {\cf19 break};\par
00208                 {\cf19 case} {\cf22 'Priority'}:\par
00209                 {\cf19 case} {\cf22 'SMTPDebug'}:\par
00210                 {\cf19 case} {\cf22 'WordWrap'}:\par
00211                     $mailer->$key = (int) $value;\par
00212                     {\cf19 break};\par
00213                 {\cf19 default}:\par
00214                     $mailer->$key = $value;\par
