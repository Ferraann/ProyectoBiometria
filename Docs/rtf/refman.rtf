00021     $sqlFinalizar = {\cf22 "UPDATE usuario_sensor }\par
00022 {\cf22                      SET actual = 0, fin_relacion = NOW() }\par
00023 {\cf22                      WHERE sensor_id = ? AND actual = 1"};\par
00024     $stmtFin = $conn->prepare($sqlFinalizar);\par
00025     $stmtFin->bind_param({\cf22 "i"}, $sensor_id);\par
00026     $stmtFin->execute();\par
00027 \par
00028     {\cf20 // 2. Marcar el sensor como con problema}\par
00029     $sqlProblema = {\cf22 "UPDATE sensor SET problema = 1 WHERE id = ?"};\par
