00003 {\cf20 // Fichero: loginUsuario.php}\par
00004 {\cf20 // Autor: Manuel}\par
00005 {\cf20 // Fecha: 5/12/2024}\par
00006 {\cf20 // Descripci\'F3n: }\par
00007 {\cf20 // Autentica un usuario verificando sus credenciales}\par
00008 {\cf20 // (correo y contrase\'F1a) contra la base de datos,}\par
00009 {\cf20 // valida que la cuenta est\'E9 activa y establece la}\par
00010 {\cf20 // sesi\'F3n si el login es exitoso.}\par
00011 {\cf20 // ------------------------------------------------------------------}\par
00012 \par
00013 {\cf17 function} loginUsuario($conn, $gmail, $password)\par
00014 \{\par
00015     {\cf20 // Inicia la sesi\'F3n solo si no est\'E1 ya iniciada}\par
00016     {\cf19 if} (session_status() === PHP_SESSION_NONE) \{\par
00017         session_start();\par
00018     \}\par
00019 \par
00020     {\cf20 /* 1. Obtener datos del usuario */}\par
00021     $stmt = $conn->prepare(\par
00022         {\cf22 "SELECT id, nombre, apellidos, gmail, password, activo}\par
00023 {\cf22          FROM usuario}\par
00024 {\cf22          WHERE gmail = ?"}\par
00025     );\par
00026     \par
00027     {\cf19 if} (!$stmt) \{\par
00028         {\cf19 return} [{\cf22 "status"} => {\cf22 "error"}, {\cf22 "mensaje"} => {\cf22 "Error en la base de datos"}];\par
00029     \}\par
00030     \par
00031     $stmt->bind_param({\cf22 "s"}, $gmail);\par
00032     $stmt->execute();\par
00033     $res = $stmt->get_result();\par
00034 \par
00035     {\cf19 if} ($res->num_rows === 0) \{\par
00036         {\cf19 return} [{\cf22 "status"} => {\cf22 "error"}, {\cf22 "mensaje"} => {\cf22 "Usuario no encontrado"}];\par
00037     \}\par
00038     \par
00039     $user = $res->fetch_assoc();\par
00040     $stmt->close();\par
00041 \par
00042     {\cf20 /* 2. Verificar contrase\'F1a */}\par
00043     {\cf19 if} (!password_verify($password, $user[{\cf22 'password'}])) \{\par
00044         {\cf19 return} [{\cf22 "status"} => {\cf22 "error"}, {\cf22 "mensaje"} => {\cf22 "Contrase\'F1a incorrecta"}];\par
00045     \}\par
00046 \par
00047     {\cf20 /* 3. Comprobar si la cuenta est\'E1 activa */}\par
00048     {\cf19 if} (!$user[{\cf22 'activo'}]) \{\par
00049         {\cf19 return} [{\cf22 "status"} => {\cf22 "error"}, {\cf22 "mensaje"} => {\cf22 "Cuenta no activada"}];\par
00050     \}\par
00051 \par
00052     {\cf20 /* 4. Establecer datos de sesi\'F3n (no guardar contrase\'F1a en texto plano) */}\par
00053     $_SESSION[{\cf22 'usuario_id'}] = $user[{\cf22 'id'}];\par
00054     $_SESSION[{\cf22 'usuario_nombre'}] = $user[{\cf22 'nombre'}];\par
00055     $_SESSION[{\cf22 'usuario_apellidos'}] = $user[{\cf22 'apellidos'}];\par
00056     $_SESSION[{\cf22 'usuario_correo'}] = $user[{\cf22 'gmail'}];\par
00057 \par
00058     unset($user[{\cf22 'password'}]);\par
00059     {\cf19 return} [{\cf22 "status"} => {\cf22 "ok"}, {\cf22 "usuario"} => $user];\par
00060 \}\par
}
\par \pard\plain 

