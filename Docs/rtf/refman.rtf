\pard\plain \s40\li0\widctlpar\adjustright \shading1000\cbpat8 \f2\fs16\cgrid 00001 <?php\par
00002 {\cf20 // ------------------------------------------------------------------}\par
00003 {\cf20 // Fichero: registrarUsuario.php}\par
00004 {\cf20 // Autor: Manuel}\par
00005 {\cf20 // Fecha: 5/12/2025}\par
00006 {\cf20 // ------------------------------------------------------------------}\par
00007 {\cf20 // Descripci\'F3n:}\par
00008 {\cf20 //  Funci\'F3n responsable del registro de usuarios. Valida datos,}\par
00009 {\cf20 //  verifica el correo, genera token y env\'EDa email de activaci\'F3n.}\par
00010 {\cf20 // ------------------------------------------------------------------}\par
00011 \par
00012 require_once __DIR__ . {\cf22 '/mailer.php'};\par
00013 use PHPMailer\\PHPMailer\\PHPMailer;\par
00014 use PHPMailer\\PHPMailer\\Exception;\par
00015 \par
00016 {\cf17 function} registrarUsuario($conn, $data)\par
00017 \{\par
00018     {\cf20 /* ---------- 1. Validaciones y saneado ---------- */}\par
00019     {\cf19 if} (!isset($data[{\cf22 'nombre'}], $data[{\cf22 'apellidos'}], $data[{\cf22 'gmail'}], $data[{\cf22 'password'}])) \{\par
00020         {\cf19 return} [{\cf22 "status"} => {\cf22 "error"}, {\cf22 "mensaje"} => {\cf22 "Faltan datos obligatorios para el registro."}];\par
00021     \}\par
00022 \par
00023     $nombre    = trim($data[{\cf22 'nombre'}]);\par
00024     $apellidos = trim($data[{\cf22 'apellidos'}]);\par
00025     $gmail     = trim($data[{\cf22 'gmail'}]);\par
00026     $password  = trim($data[{\cf22 'password'}]);\par
00027     \par
00028     {\cf20 // Validar formato de correo}\par
00029     {\cf19 if} (!filter_var($gmail, FILTER_VALIDATE_EMAIL)) \{\par
00030         {\cf19 return} [{\cf22 "status"} => {\cf22 "error"}, {\cf22 "mensaje"} => {\cf22 "El formato del correo electr\'F3nico no es v\'E1lido."}];\par
00031     \}\par
00032     \par
00033     $hash      = password_hash($password, PASSWORD_DEFAULT);\par
00034 \par
00035     {\cf20 /* ---------- 2. \'BFExiste el correo? ---------- */}\par
00036     $stmt = $conn->prepare({\cf22 "SELECT id, activo FROM usuario WHERE gmail = ?"});\par
00037     $stmt->bind_param({\cf22 "s"}, $gmail);\par
00038     $stmt->execute();\par
00039     $res = $stmt->get_result();\par
00040 \par
00041     {\cf19 if} ($res->num_rows) \{\par
00042         $usr = $res->fetch_assoc();\par
00043 \par
00044         {\cf19 if} ($usr[{\cf22 'activo'}] == 1) \{\par
00045             {\cf19 return} [{\cf22 "status"} => {\cf22 "error"}, {\cf22 "mensaje"} => {\cf22 "El correo ya est\'E1 registrado y activado."}];\par
00046         \}\par
00047 \par
00048         {\cf20 /* usuario inactivo \'E2\'86\'92 lo borramos */}\par
00049         $del = $conn->prepare({\cf22 "DELETE FROM usuario WHERE id = ?"});\par
00050         $del->bind_param({\cf22 "i"}, $usr[{\cf22 'id'}]);\par
00051         $del->execute();\par
00052     \}\par
00053 \par
00054     {\cf20 /* ---------- 3. Crear token seguro ---------- */}\par
00055     $token = bin2hex(random_bytes(32)); {\cf20 // 64 caracteres}\par
00056     $token_expira = date({\cf22 "Y-m-d H:i:s"}, time() + 900); {\cf20 // 15 min de validez}\par
00057 \par
00058     {\cf20 /* ---------- 4. Insertar nuevo usuario (inactivo + token + expiraci\'F3n) ---------- */}\par
00059     $ins = $conn->prepare({\cf22 "INSERT INTO usuario }\par
00060 {\cf22         (nombre, apellidos, gmail, password, activo, token, token_expira)}\par
00061 {\cf22         VALUES (?, ?, ?, ?, 0, ?, ?)"});\par
00062     $ins->bind_param({\cf22 "ssssss"}, $nombre, $apellidos, $gmail, $hash, $token, $token_expira);\par
00063 \par
00064     {\cf19 if} (!$ins->execute()) \{\par
00065         {\cf19 return} [{\cf22 "status"} => {\cf22 "error"}, {\cf22 "mensaje"} => {\cf22 "Error al registrar el usuario: "} . $conn->error];\par
00066     \}\par
00067 \par
00068     {\cf20 /* ---------- 5. Enviar correo de activaci\'F3n ---------- */}\par
00069     $enlace = {\cf22 "https://fsanpra.upv.edu.es/src/html/activacion.html?token="} . urlencode($token);\par
00070 \par
00071     $asunto  = {\cf22 "Activa tu cuenta en AITHER"};\par
00072     $cuerpo  = {\cf22 "<h2>\'A1Hola $nombre!</h2>}\par
00073 {\cf22         <p>Gracias por registrarte. Pulsa el bot\'F3n para activar tu cuenta:</p>}\par
00074 {\cf22         <p><a href='$enlace' style='background:#007bff;color:white;padding:10px 20px;text-decoration:none;border-radius:4px;'>Activar cuenta</a></p>}\par
00075 {\cf22         <p>Si el bot\'F3n no funciona, copia y pega esta direcci\'F3n:<br>$enlace</p>"};\par
00076 \par
00077     $enviado = {\cf17 false};\par
00078 \par
00079     {\cf19 try} \{\par
00080         $mail = {\cf17 new} PHPMailer({\cf17 true});\par
00081         $mail->isSMTP();\par
00082         $mail->Host       = {\cf22 'smtp.gmail.com'};\par
00083         $mail->SMTPAuth   = {\cf17 true};\par
00084         $mail->Username   = {\cf22 'no.reply.aither@gmail.com'};\par
00085         $mail->Password   = {\cf22 'esdf lkoc qprz rkum'};\par
00086         $mail->SMTPSecure = PHPMailer::ENCRYPTION_STARTTLS;\par
00087         $mail->Port       = 587;\par
00088 \par
00089         $mail->setFrom({\cf22 'noreply@aither.com'}, {\cf22 'AITHER'});\par
00090         $mail->addAddress($gmail);\par
00091         $mail->isHTML({\cf17 true});\par
00092         $mail->Subject = $asunto;\par
00093         $mail->Body    = $cuerpo;\par
00094 \par
00095         $mail->send();\par
00096         $enviado = {\cf17 true};\par
00097 \par
00098     \} {\cf19 catch} (Exception $e) \{\par
00099         error_log({\cf22 'PHPMailer error: '} . $mail->ErrorInfo);\par
00100         {\cf19 return} [{\cf22 "status"} => {\cf22 "ok"}, {\cf22 "mensaje"} => {\cf22 "Usuario registrado correctamente, pero no se pudo enviar el correo de activaci\'F3n. Contacta con soporte."}];\par
00101     \}\par
00102 \par
00103     {\cf19 return} [{\cf22 "status"} => {\cf22 "ok"}, {\cf22 "mensaje"} => {\cf22 "Usuario registrado correctamente. Revisa tu correo para activarlo."}];\par
00104 \}\par
00105 ?>\par
}
