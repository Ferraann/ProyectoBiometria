00024 {\cf20 //  Clase donde a traves del formulario mostrado en el layout,}\par
00025 {\cf20 //  se enviara los datos a traves de Retrofit al servidor web.}\par
00026 {\cf20 //  Donde se guardara y se usara posteriormente.}\par
00027 {\cf20 // ------------------------------------------------------------------}\par
00028 {\cf17 public} {\cf17 class }RegistroActivity {\cf17 extends} AppCompatActivity \{\par
00029     {\cf20 //Formulario del layout donde se trabaja}\par
00030     EditText Usuario,Apellidos,Email,Contrasenya,RepetirContrasenya;\par
00031 \par
00032     {\cf20 //Regex que me permite confirma que la contrase\'F1a es segura.}\par
00033     String regexTieneMayuscula = {\cf22 "^(?=.*[A-Z]).+$"};\par
00034     String regexTieneNumeros = {\cf22 "^(?=.*\\\\d).+$"};\par
00035     String regexTieneSimbologia = {\cf22 "^(?=.*[$@\'80!%*?&]).+$"};\par
00036     String regexTieneMasDe8Caracteres = {\cf22 "^.\{8,\}$"};\par
00037 \par
00038     {\cf20 //Metodo onCreate donde se ejecuta lo principal}\par
00039     {\cf17 protected} {\cf18 void} onCreate(Bundle savedInstanceState) \{\par
00040         super.onCreate(savedInstanceState);\par
00041         setContentView(R.layout.activity_registro);\par
00042 \par
00043         {\cf20 //Obtengo los edit text por su id que yo le he piesto}\par
00044         Usuario = findViewById(R.id.NombreUsuario);\par
00045         Apellidos = findViewById(R.id.ApellidosUsuario);\par
00046         Email = findViewById(R.id.EmailUsuario);\par
00047         Contrasenya = findViewById(R.id.ContrasenyaUsuario);\par
00048         RepetirContrasenya = findViewById(R.id.RepetirContrasenyaUsuario);\par
00049 \par
00050         verificarContrasenya();\par
00051     \}\par
00052 \par
00053     {\cf20 //Boton para enviar los datos al servidor}\par
00054     {\cf17 public} {\cf18 void} botonEnviarDatos(View v)\{\par
00055         String usuario = Usuario.getText().toString().trim();\par
00056         String apellidos = Apellidos.getText().toString().trim();\par
00057         String contrasenya =Contrasenya.getText().toString().trim();\par
00058         String email = Email.getText().toString().trim();\par
00059         String repetirContrasenya = RepetirContrasenya.getText().toString().trim();\par
00060 \par
00061 \par
00062         {\cf20 // validaci\'F3n que comprueba que si hay alguno que est\'E1 vacio no se ejecuta la sentencia sql.}\par
00063         {\cf19 if} (usuario.isEmpty() || apellidos.isEmpty()||contrasenya.isEmpty()||email.isEmpty()||repetirContrasenya.isEmpty()) \{\par
00064             Toast.makeText({\cf17 this}, {\cf22 "Por favor, rellena todos los campos"}, Toast.LENGTH_SHORT).show();\par
00065             {\cf19 return};\par
00066         \}\par
00067 \par
00068         {\cf20 //Con una biblioteca podemos comprobar la estructura basica de un email}\par
00069         {\cf20 //si est\'E1 detecta que no es semejante a un email, entonces devuelve false.}\par
00070         {\cf20 //si es verdad entonces un true.}\par
00071         {\cf19 if} (!Patterns.EMAIL_ADDRESS.matcher(email).matches())\{\par
00072             Toast.makeText({\cf17 this}, {\cf22 "Por favor, introduce un email valido"}, Toast.LENGTH_SHORT).show();\par
00073             {\cf19 return};\par
00074         \}\par
00075 \par
00076         {\cf19 if}(!contrasenya.matches(repetirContrasenya))\{\par
00077             Toast.makeText({\cf17 this},{\cf22 "Por favor, repita correctamente la contrase\'F1a"}, Toast.LENGTH_SHORT).show();\par
00078             {\cf19 return};\par
00079         \}\par
00080 \par
00081         {\cf19 if} (!contrasenya.matches(regexTieneMasDe8Caracteres)||!contrasenya.matches(regexTieneNumeros)||!contrasenya.matches(regexTieneMayuscula)||!contrasenya.matches(regexTieneSimbologia))\{\par
00082             Toast.makeText({\cf17 this}, {\cf22 "Por favor, introduce una contrase\'F1a segura"}, Toast.LENGTH_SHORT).show();\par
00083             {\cf19 return};\par
00084         \}\par
00085 \par
00086         PostRegistro(Usuario.getText().toString(),Apellidos.getText().toString(),Email.getText().toString(),Contrasenya.getText().toString(),{\cf17 this});\par
00087 \par
00088     \}\par
00089 \par
00090     {\cf17 public} {\cf18 void} verificarContrasenya()\{\par
00091         Contrasenya.addTextChangedListener({\cf17 new} TextWatcher() \{\par
00092             @Override\par
00093             {\cf17 public} {\cf18 void} beforeTextChanged(CharSequence s, {\cf18 int} start, {\cf18 int} count, {\cf18 int} after) \{\par
00094 \par
00095             \}\par
00096 \par
00097             @Override\par
00098             {\cf17 public} {\cf18 void} onTextChanged(CharSequence s, {\cf18 int} start, {\cf18 int} before, {\cf18 int} count) \{\par
00099                 {\cf19 if}(Contrasenya.getText().toString().matches(regexTieneMayuscula))\{\par
00100                     Log.d({\cf22 "Regex"},{\cf22 "Tiene mayusculas"});\par
00101                 \}\par
00102                 {\cf19 if}(Contrasenya.getText().toString().matches(regexTieneNumeros))\{\par
00103                     Log.d({\cf22 "Regex"},{\cf22 "Tiene numeros"});\par
00104                 \}\par
00105                 {\cf19 if}(Contrasenya.getText().toString().matches(regexTieneMasDe8Caracteres))\{\par
00106                     Log.d({\cf22 "Regex"},{\cf22 "Tiene mas de 8 caracteres"});\par
00107                 \}\par
00108                 {\cf19 if}(Contrasenya.getText().toString().matches(regexTieneSimbologia))\{\par
00109                     Log.d({\cf22 "Regex"},{\cf22 "Tiene simbologia"});\par
00110                 \}\par
00111             \}\par
00112 \par
00113             @Override\par
00114             {\cf17 public} {\cf18 void} afterTextChanged(Editable s) \{\par
00115 \par
00116             \}\par
00117         \});\par
00118     \}\par
00119 \}\par
}
