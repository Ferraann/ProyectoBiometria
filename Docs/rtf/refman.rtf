\pard\plain \s40\li0\widctlpar\adjustright \shading1000\cbpat8 \f2\fs16\cgrid 00001 {\cf17 package }com.example.grupo5.aitherapp.retrofit;\par
00002 \par
00003 {\cf17 import} okhttp3.OkHttpClient;\par
00004 {\cf17 import} okhttp3.logging.HttpLoggingInterceptor;\par
00005 {\cf17 import} retrofit2.Retrofit;\par
00006 {\cf17 import} retrofit2.converter.gson.GsonConverterFactory;\par
00007 \par
00008 {\cf20 // ------------------------------------------------------------------}\par
00009 {\cf20 // Fichero: ApiCliente}\par
00010 {\cf20 // Autor: Pablo Chasi}\par
00011 {\cf20 // Fecha: 24/10/2025}\par
00012 {\cf20 // ------------------------------------------------------------------}\par
00013 {\cf20 // Clase ApiCliente}\par
00014 {\cf20 //}\par
00015 {\cf20 // Descripci\'F3n:}\par
00016 {\cf20 //   Clase que se encarga de configurar y crear una estancia}\par
00017 {\cf20 //  de Retrofit cliente que permite conectar tu aplicaci\'F3n}\par
00018 {\cf20 //  Android con un servidor web o Api. Ademas, se declararan los metodos}\par
00019 {\cf20 //  de las apps.}\par
00020 {\cf20 // ------------------------------------------------------------------}\par
00021 {\cf17 public} {\cf17 class }ApiCliente \{\par
00022     {\cf20 //Declaro mi ip}\par
00023     {\cf17 private} {\cf17 static} {\cf17 final} String urlLocal = {\cf22 "http://172.25.181.250/ProyectoBiometria_2/src/api/"};\par
00024 \par
00025     {\cf20 //Pongo Retrofit en null para despu\'E9s comprobar si est\'E1 o no funcional}\par
00026     {\cf17 private} {\cf17 static} Retrofit retrofit = {\cf17 null};\par
00027 \par
00028     {\cf20 //-------------------------------------------------}\par
00029     {\cf20 //      getApiService() --> retrofit}\par
00030     {\cf20 //-------------------------------------------------}\par
00031     {\cf17 public} {\cf17 static} ApiService getApiService()\{\par
00032         {\cf19 if} (retrofit == {\cf17 null}) \{\par
00033             {\cf20 //interceptar metodo php}\par
00034             HttpLoggingInterceptor interceptor = {\cf17 new} HttpLoggingInterceptor();\par
00035             interceptor.setLevel(HttpLoggingInterceptor.Level.BODY);\par
00036 \par
00037             {\cf20 //Consigo el cliente para enviarlo}\par
00038             OkHttpClient client = {\cf17 new} OkHttpClient.Builder().addInterceptor(interceptor).build();\par
00039 \par
00040             {\cf20 //Declaro mi estructura para despu\'E9s saber donde envio los datos}\par
00041             retrofit = {\cf17 new} Retrofit.Builder()\par
00042                     .baseUrl(urlLocal)\par
00043                     .addConverterFactory(GsonConverterFactory.create())\par
00044                     .client(client)\par
00045                     .build();\par
00046         \}\par
00047         {\cf19 return} retrofit.create(ApiService.class);\par
00048     \}\par
00049 \par
00050 \}\par
}
