00041 {\cf20 // ------------------------------------------------------------------}\par
00042 {\cf20 // Descripci\'F3n:}\par
00043 {\cf20 // Clase encargada de gestionar la lectura de c\'F3digos QR mediante la}\par
00044 {\cf20 // c\'E1mara del dispositivo usando la librer\'EDa CameraX junto con el}\par
00045 {\cf20 // lector de QR de ML Kit. Su objetivo es vincular un sensor a un}\par
00046 {\cf20 // usuario, extrayendo los datos del QR (MAC y modelo), verificando}\par
00047 {\cf20 // que el usuario est\'E9 logueado y enviando dicha informaci\'F3n a la API.}\par
00048 {\cf20 //}\par
00049 {\cf20 // Flujo principal:}\par
00050 {\cf20 //  1. Solicitar permiso de c\'E1mara.}\par
00051 {\cf20 //  2. Activar CameraX y procesar cada frame.}\par
00052 {\cf20 //  3. Leer QR, interpretar JSON y extraer datos.}\par
00053 {\cf20 //  4. Validar usuario y datos recibidos.}\par
00054 {\cf20 //  5. Enviar el sensor a la base de datos con postVincularSensor().}\par
00055 {\cf20 // ------------------------------------------------------------------}\par
00056 {\cf17 public} {\cf17 class }VincularQRActivity {\cf17 extends} AppCompatActivity \{\par
00057     {\cf17 private} {\cf17 static} {\cf17 final} {\cf18 int} REQUEST_CAMERA_PERMISSION = 1001;\par
00058 \par
00059     {\cf17 private} PreviewView previewView;\par
00060     {\cf17 private} TextView textResult;\par
00061 \par
00062     {\cf17 private} {\cf18 boolean} qrProcesado = {\cf17 false};\par
00063 \par
00064     @Override\par
00065     {\cf17 protected} {\cf18 void} onCreate(Bundle savedInstanceState) \{\par
00066         super.onCreate(savedInstanceState);\par
00067         setContentView(R.layout.activity_vincular_qr);\par
00068 \par
00069         previewView = findViewById(R.id.previewView);\par
00070         textResult = findViewById(R.id.textResult);\par
00071 \par
00072         {\cf19 if} (allPermissionsGranted()) \{\par
00073             startCamera();\par
00074         \} {\cf19 else} \{\par
00075             ActivityCompat.requestPermissions({\cf17 this},\par
00076                     {\cf17 new} String[]\{Manifest.permission.CAMERA\},\par
00077                     REQUEST_CAMERA_PERMISSION);\par
00078         \}\par
00079     \}\par
00080 \par
00081     {\cf17 private} {\cf18 boolean} allPermissionsGranted() \{\par
00082         {\cf19 return} ContextCompat.checkSelfPermission({\cf17 this}, Manifest.permission.CAMERA)\par
00083                 == PackageManager.PERMISSION_GRANTED;\par
00084     \}\par
00085 \par
00086     @Override\par
00087     {\cf17 public} {\cf18 void} onRequestPermissionsResult({\cf18 int} requestCode, @NonNull String[] permissions,\par
00088                                            @NonNull {\cf18 int}[] grantResults) \{\par
00089         super.onRequestPermissionsResult(requestCode, permissions, grantResults);\par
00090         {\cf19 if} (requestCode == REQUEST_CAMERA_PERMISSION) \{\par
00091             {\cf19 if} (allPermissionsGranted()) \{\par
00092                 startCamera();\par
00093             \} {\cf19 else} \{\par
00094                 Toast.makeText({\cf17 this}, {\cf22 "Permiso de c\'E1mara requerido"}, Toast.LENGTH_SHORT).show();\par
00095                 finish();\par
00096             \}\par
00097         \}\par
00098     \}\par
00099 \par
00100     {\cf17 private} {\cf18 void} startCamera() \{\par
00101         ListenableFuture<ProcessCameraProvider> cameraProviderFuture =\par
00102                 ProcessCameraProvider.getInstance({\cf17 this});\par
00103 \par
00104         cameraProviderFuture.addListener(() -> \{\par
00105             {\cf19 try} \{\par
00106                 ProcessCameraProvider cameraProvider = cameraProviderFuture.get();\par
00107 \par
00108                 Preview preview = {\cf17 new} Preview.Builder().build();\par
00109                 preview.setSurfaceProvider(previewView.getSurfaceProvider());\par
00110 \par
00111                 ImageAnalysis imageAnalysis = {\cf17 new} ImageAnalysis.Builder()\par
00112                         .setBackpressureStrategy(ImageAnalysis.STRATEGY_KEEP_ONLY_LATEST)\par
00113                         .build();\par
00114 \par
00115                 BarcodeScannerOptions options = {\cf17 new} BarcodeScannerOptions.Builder()\par
00116                         .setBarcodeFormats(Barcode.FORMAT_QR_CODE)\par
00117                         .build();\par
00118 \par
00119                 BarcodeScanner scanner = BarcodeScanning.getClient(options);\par
00120 \par
00121                 imageAnalysis.setAnalyzer(ContextCompat.getMainExecutor({\cf17 this}), imageProxy -> \{\par
00122                     processImageProxy(scanner, imageProxy, this);\par
00123                 \});\par
00124 \par
00125                 CameraSelector cameraSelector = CameraSelector.DEFAULT_BACK_CAMERA;\par
00126 \par
00127                 cameraProvider.unbindAll();\par
00128                 cameraProvider.bindToLifecycle({\cf17 this}, cameraSelector, preview, imageAnalysis);\par
00129 \par
00130             \} {\cf19 catch} (ExecutionException | InterruptedException e) \{\par
00131                 e.printStackTrace();\par
00132             \}\par
00133         \}, ContextCompat.getMainExecutor({\cf17 this}));\par
00134     \}\par
00135 \par
00136     @OptIn(markerClass = ExperimentalGetImage.class)\par
00137     {\cf17 private} {\cf18 void} processImageProxy(BarcodeScanner scanner, ImageProxy imageProxy, Context context) \{\par
00138         {\cf19 if} (imageProxy.getImage() == {\cf17 null}) \{\par
00139             imageProxy.close();\par
00140             {\cf19 return};\par
00141         \}\par
00142 \par
00143         InputImage image = InputImage.fromMediaImage(\par
00144                 imageProxy.getImage(),\par
00145                 imageProxy.getImageInfo().getRotationDegrees()\par
00146         );\par
00147 \par
00148         scanner.process(image)\par
00149                 .addOnSuccessListener(barcodes -> \{\par
00150                     {\cf19 if} (qrProcesado) {\cf19 return}; {\cf20 // ya procesado, ignoramos}\par
00151                     {\cf19 for} (Barcode barcode : barcodes) \{\par
00152                         String raw = barcode.getRawValue();\par
00153                         {\cf19 if} (raw == {\cf17 null}) {\cf19 continue};\par
00154 \par
00155                         qrProcesado = {\cf17 true}; {\cf20 // bloqueamos m\'E1s lecturas}\par
00156 \par
00157                         QrData qr;\par
00158                         {\cf19 try} \{\par
00159                             qr = {\cf17 new} Gson().fromJson(raw, QrData.class);\par
00160                         \} {\cf19 catch} (Exception e) \{\par
00161                             Toast.makeText(context, {\cf22 "QR no v\'E1lido"}, Toast.LENGTH_SHORT).show();\par
00162                             {\cf19 continue};\par
00163                         \}\par
00164 \par
00165                         {\cf19 if} (qr.mac == {\cf17 null} || qr.mac.isEmpty()) \{\par
00166                             Toast.makeText(context, {\cf22 "Falta la MAC en el QR"}, Toast.LENGTH_SHORT).show();\par
00167                             {\cf19 continue};\par
00168                         \}\par
00169 \par
00170                         SharedPreferences prefs = context.getSharedPreferences({\cf22 "SesionUsuario"}, MODE_PRIVATE);\par
00171                         {\cf18 int} idUsuario = Integer.parseInt(prefs.getString({\cf22 "id"},{\cf22 ""}));\par
00172                         {\cf19 if} (idUsuario <= 0) \{\par
00173                             Toast.makeText(context, {\cf22 "Usuario no logueado"}, Toast.LENGTH_SHORT).show();\par
00174                             {\cf19 continue};\par
00175                         \}\par
00176 \par
00177                         textResult.setText({\cf22 "QR detectado: "} + raw);\par
00178                         PojoSensor sensor = {\cf17 new} PojoSensor();\par
00179                         sensor.setAccion({\cf22 "crearSensorYRelacion"});\par
00180                         sensor.setMac(qr.mac);\par
00181                         sensor.setModelo(qr.modelo);\par
00182                         sensor.setUsuario_id(String.valueOf(idUsuario));\par
00183 \par
00184                         postVincularSensor(sensor, {\cf17 this});\par
00185 \par
00186                         {\cf19 break}; {\cf20 // salimos del for}\par
00187                     \}\par
00188                 \})\par
00189                 .addOnFailureListener(Throwable::printStackTrace)\par
00190                 .addOnCompleteListener(task -> imageProxy.close());\par
00191 \par
00192     \}\par
00193 \par
00194     {\cf20 /* Clase auxiliar para parsear el JSON del QR */}\par
00195     {\cf17 private} {\cf17 static} {\cf17 class }QrData \{\par
00196         String mac;\par
00197         String modelo;\par
00198     \}\par
00199 \}\par
}
