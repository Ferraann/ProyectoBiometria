00081 {\cf22 }\par
00082 {\cf22 $providerName = '}{\cf22 ';}\par
00083 {\cf22 $clientId = '}{\cf22 ';}\par
00084 {\cf22 $clientSecret = '}{\cf22 ';}\par
00085 {\cf22 $tenantId = '}{\cf22 ';}\par
00086 {\cf22 }\par
00087 {\cf22 if (array_key_exists('}provider{\cf22 ', $_POST)) \{}\par
00088 {\cf22     $providerName = $_POST['}provider{\cf22 '];}\par
00089 {\cf22     $clientId = $_POST['}clientId{\cf22 '];}\par
00090 {\cf22     $clientSecret = $_POST['}clientSecret{\cf22 '];}\par
00091 {\cf22     $tenantId = $_POST['}tenantId{\cf22 '];}\par
00092 {\cf22     $_SESSION['}provider{\cf22 '] = $providerName;}\par
00093 {\cf22     $_SESSION['}clientId{\cf22 '] = $clientId;}\par
00094 {\cf22     $_SESSION['}clientSecret{\cf22 '] = $clientSecret;}\par
00095 {\cf22     $_SESSION['}tenantId{\cf22 '] = $tenantId;}\par
00096 {\cf22 \} elseif (array_key_exists('}provider{\cf22 ', $_SESSION)) \{}\par
00097 {\cf22     $providerName = $_SESSION['}provider{\cf22 '];}\par
00098 {\cf22     $clientId = $_SESSION['}clientId{\cf22 '];}\par
00099 {\cf22     $clientSecret = $_SESSION['}clientSecret{\cf22 '];}\par
00100 {\cf22     $tenantId = $_SESSION['}tenantId{\cf22 '];}\par
00101 {\cf22 \}}\par
00102 {\cf22 }\par
00103 {\cf22 //If you don'}t want to use the built-in form, {\cf17 set} your client {\cf18 id} and secret here\par
00104 {\cf20 //$clientId = 'RANDOMCHARS-----duv1n2.apps.googleusercontent.com';}\par
00105 {\cf20 //$clientSecret = 'RANDOMCHARS-----lGyjPcRtvP';}\par
00106 \par
00107 {\cf20 //If this automatic URL doesn't work, set it yourself manually to the URL of this script}\par
00108 $redirectUri = (isset($_SERVER[{\cf22 'HTTPS'}]) ? {\cf22 'https://'} : {\cf22 'http://'}) . $_SERVER[{\cf22 'HTTP_HOST'}] . $_SERVER[{\cf22 'PHP_SELF'}];\par
00109 {\cf20 //$redirectUri = 'http://localhost/PHPMailer/redirect';}\par
00110 \par
00111 $params = [\par
00112     {\cf22 'clientId'} => $clientId,\par
00113     {\cf22 'clientSecret'} => $clientSecret,\par
00114     {\cf22 'redirectUri'} => $redirectUri,\par
00115     {\cf22 'accessType'} => {\cf22 'offline'}\par
00116 ];\par
00117 \par
00118 $options = [];\par
00119 $provider = {\cf17 null};\par
00120 \par
00121 {\cf19 switch} ($providerName) \{\par
00122     {\cf19 case} {\cf22 'Google'}:\par
00123         $provider = {\cf17 new} Google($params);\par
00124         $options = [\par
00125             {\cf22 'scope'} => [\par
00126                 {\cf22 'https://mail.google.com/'}\par
00127             ]\par
00128         ];\par
00129         {\cf19 break};\par
00130     {\cf19 case} {\cf22 'Yahoo'}:\par
00131         $provider = {\cf17 new} Yahoo($params);\par
00132         {\cf19 break};\par
00133     {\cf19 case} {\cf22 'Microsoft'}:\par
00134         $provider = {\cf17 new} Microsoft($params);\par
00135         $options = [\par
00136             {\cf22 'scope'} => [\par
00137                 {\cf22 'wl.imap'},\par
00138                 {\cf22 'wl.offline_access'}\par
00139             ]\par
00140         ];\par
00141         {\cf19 break};\par
00142     {\cf19 case} {\cf22 'Azure'}:\par
00143         $params[{\cf22 'tenantId'}] = $tenantId;\par
00144 \par
00145         $provider = {\cf17 new} Azure($params);\par
00146         $options = [\par
00147             {\cf22 'scope'} => [\par
00148                 {\cf22 'https://outlook.office.com/SMTP.Send'},\par
00149                 {\cf22 'offline_access'}\par
00150             ]\par
00151         ];\par
00152         {\cf19 break};\par
00153 \}\par
00154 \par
00155 {\cf19 if} ({\cf17 null} === $provider) \{\par
00156     exit({\cf22 'Provider missing'});\par
00157 \}\par
00158 \par
00159 {\cf19 if} (!isset($_GET[{\cf22 'code'}])) \{\par
00160     {\cf20 //If we don't have an authorization code then get one}\par
00161     $authUrl = $provider->getAuthorizationUrl($options);\par
00162     $_SESSION[{\cf22 'oauth2state'}] = $provider->getState();\par
00163     header({\cf22 'Location: '} . $authUrl);\par
00164     exit;\par
00165     {\cf20 //Check given state against previously stored one to mitigate CSRF attack}\par
00166 \} elseif (empty($_GET[{\cf22 'state'}]) || ($_GET[{\cf22 'state'}] !== $_SESSION[{\cf22 'oauth2state'}])) \{\par
00167     unset($_SESSION[{\cf22 'oauth2state'}]);\par
00168     unset($_SESSION[{\cf22 'provider'}]);\par
00169     exit({\cf22 'Invalid state'});\par
00170 \} {\cf19 else} \{\par
00171     unset($_SESSION[{\cf22 'provider'}]);\par
00172     {\cf20 //Try to get an access token (using the authorization code grant)}\par
00173     $token = $provider->getAccessToken(\par
00174         {\cf22 'authorization_code'},\par
00175         [\par
00176             {\cf22 'code'} => $_GET[{\cf22 'code'}]\par
00177         ]\par
00178     );\par
00179     {\cf20 //Use this to interact with an API on the users behalf}\par
00180     {\cf20 //Use this to get a new access token if the old one expires}\par
00181     echo {\cf22 'Refresh Token: '}, htmlspecialchars($token->getRefreshToken());\par
00182 \}\par
}
