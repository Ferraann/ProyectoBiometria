{\bkmkstart AAAAAAAAEN}
{\bkmkend AAAAAAAAEN}
{
\par
\pard\plain \s40\li0\widctlpar\adjustright \shading1000\cbpat8 \f2\fs16\cgrid 00001 <?php\par
00002 \par
00003 {\cf20 // ------------------------------------------------------------------}\par
00004 {\cf20 // Fichero: mainTest1.php (CORREGIDO)}\par
00005 {\cf20 // Autor: Ferran Sansaloni Prats}\par
00006 {\cf20 // Fecha: 30/10/2025}\par
00007 {\cf20 // ------------------------------------------------------------------}\par
00008 {\cf20 // Descripci\'F3n:}\par
00009 {\cf20 //   Script para probar la l\'F3gica de negocio con la funci\'F3n registrarUsuario.}\par
00010 {\cf20 //     1. Inserta un usuario de prueba pasando un array de datos.}\par
00011 {\cf20 //     2. Recupera los usuarios con el correo de prueba.}\par
00012 {\cf20 //     3. Muestra el contenido recuperado en formato JSON.}\par
00013 {\cf20 //     4. Limpieza: elimina el usuario de prueba para mantener la idempotencia.}\par
00014 {\cf20 // ------------------------------------------------------------------}\par
00015 \par
00016 {\cf20 // ------------------------------------------------------------------}\par
00017 {\cf20 // INCLUDES NECESARIOS}\par
00018 {\cf20 // ------------------------------------------------------------------}\par
00019 require_once({\cf22 '../api/conexion.php'});\par
00020 \par
00021 {\cf19 foreach} (glob(__DIR__ . {\cf22 "/../api/logicaNegocio/*.php"}) as $file) \{\par
00022     require_once $file;\par
00023 \}\par
00024 \par
00025 $conn = abrirServidor();\par
00026 \par
00027 {\cf20 // -------------------------}\par
00028 {\cf20 // TEST 1: Insertar usuario}\par
00029 {\cf20 // -------------------------}\par
00030 echo {\cf22 "Test 1: Guardar usuario --> \\t"};\par
00031 \par
00032 {\cf20 // email que usamos para el test}\par
00033 $emailTest = {\cf22 "esteeselcorreoparaeltest@gmail.com"};\par
00034 \par
00035 $datosUsuario = [\par
00036     {\cf22 'nombre'}    => {\cf22 'NombreTest'},\par
00037     {\cf22 'apellidos'} => {\cf22 'ApellidosTest'},\par
00038     {\cf22 'gmail'}     => $emailTest,\par
00039     {\cf22 'password'}  => {\cf22 '1234PruebaSegura'},\par
00040 ];\par
00041 \par
00042 {\cf20 // **2. Llamamos al metodo registrarUsuario con los 2 par\'E1metros correctos: $conn y $datosUsuario**}\par
00043 {\cf20 // La funci\'F3n devuelve un array ['status' => 'ok'/'error', 'mensaje' => '...']}\par
00044 $resultado = registrarUsuario($conn, $datosUsuario);\par
00045 \par
00046 {\cf19 if} ($resultado[{\cf22 'status'}] === {\cf22 'ok'}) \{\par
00047     {\cf20 // Todo bien}\par
00048     echo {\cf22 "Usuario insertado correctamente.\\n"};\par
00049     echo {\cf22 "Mensaje: **"} . $resultado[{\cf22 'mensaje'}] . {\cf22 "**\\n\\n"};\par
00050 \} {\cf19 else} \{\par
00051     {\cf20 // Error}\par
00052     die({\cf22 "Error al insertar el usuario.\\nMensaje: **"} . $resultado[{\cf22 'mensaje'}] . {\cf22 "**\\n\\n"});\par
00053 \}\par
00054 \par
00055 {\cf20 // =========================}\par
00056 {\cf20 // TEST 2: Leer datos del usuario}\par
00057 {\cf20 // =========================}\par
00058 echo {\cf22 "Test 2: Recoger datos del usuario --> \\n"};\par
00059 \par
00060 {\cf20 // Ejecutamos la consulta}\par
00061 $stmt = $conn->prepare({\cf22 "SELECT * FROM usuario WHERE gmail = ?"});\par
00062 $stmt->bind_param({\cf22 "s"}, $emailTest);\par
00063 $stmt->execute();\par
00064 $result = $stmt->get_result();\par
00065 \par
00066 {\cf20 // Lista para los usuarios con el correo de prueba}\par
00067 $usuarios = [];\par
00068 \par
00069 {\cf19 if} ($result && $result->num_rows > 0) \{\par
00070     {\cf19 while} ($row = $result->fetch_assoc()) \{\par
00071         {\cf20 // A\'F1adimos los usuarios encontrados en la lista}\par
00072         $usuarios[] = $row;\par
00073     \}\par
00074     {\cf20 // Todo bien}\par
00075     echo {\cf22 "Usuario(s) recuperado(s): **"} . count($usuarios) . {\cf22 "**\\n\\n"};\par
00076 \} {\cf19 else} \{\par
00077     {\cf20 // Error}\par
00078     echo {\cf22 "No se han recuperado usuarios.\\n\\n"};\par
00079 \}\par
00080 \par
00081 {\cf20 // =========================}\par
00082 {\cf20 // TEST 3: Mostrar contenido}\par
00083 {\cf20 // =========================}\par
00084 {\cf20 // Mostramos los datos}\par
00085 echo {\cf22 "Test 3: Mostrar datos --> \\n"};\par
00086 echo json_encode($usuarios, JSON_PRETTY_PRINT);\par
00087 \par
00088 echo {\cf22 "\\n\\n"};\par
00089 {\cf20 // =========================}\par
00090 {\cf20 // TEST 4: Limpieza}\par
00091 {\cf20 // =========================}\par
00092 echo {\cf22 "Test 4: Eliminar el dato insertado (idempotente) --> \\n"};\par
00093 {\cf20 // Si hay alguna usuario con ese correo electr\'F3nico que se elimine}\par
00094 {\cf19 if} (!empty($usuarios)) \{\par
00095     $delStmt = $conn->prepare({\cf22 "DELETE FROM usuario WHERE gmail = ?"});\par
00096     $delStmt->bind_param({\cf22 "s"}, $emailTest);\par
00097 \par
00098     {\cf19 if} ($delStmt->execute()) \{\par
00099         echo {\cf22 "Usuario eliminado correctamente.\\n"};\par
00100     \} {\cf19 else} \{\par
00101         echo {\cf22 "Error al eliminar usuario: "} . $conn->error . {\cf22 "\\n"};\par
00102     \}\par
00103 \} {\cf19 else} \{\par
00104     echo {\cf22 "No hay usuario para eliminar.\\n"};\par
00105 \}\par
00106 \par
00107 $conn->close();\par
00108 \par
00109 {\cf20 // ------------------------------------------------------------------}\par
00110 {\cf20 // ------------------------------------------------------------------}\par
00111 {\cf20 // ------------------------------------------------------------------}\par
00112 {\cf20 // ------------------------------------------------------------------}\par
00113 ?>\par
