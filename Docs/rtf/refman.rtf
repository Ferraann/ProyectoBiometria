00079     \{\par
00080         $config = $this->parseUrl($dsn);\par
00081 \par
00082         {\cf19 if} ({\cf17 false} === $config || !isset($config[{\cf22 'scheme'}]) || !isset($config[{\cf22 'host'}])) \{\par
00083             {\cf19 throw} {\cf17 new} Exception({\cf22 'Malformed DSN'});\par
00084         \}\par
00085 \par
00086         {\cf19 if} (isset($config[{\cf22 'query'}])) \{\par
00087             parse_str($config[{\cf22 'query'}], $config[{\cf22 'query'}]);\par
00088         \}\par
