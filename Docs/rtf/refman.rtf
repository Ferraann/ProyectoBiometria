00167 \par
00168 {\cf20 // --- INICIO DE LA VERIFICACI\'D3N DE SEGURIDAD ---}\par
00169 $passwordAntigua = $_POST[{\cf22 'contrasena-antigua'}];\par
00170 \par
00171 {\cf20 // Si se va a cambiar la contrase\'F1a, se DEBE proporcionar la contrase\'F1a antigua.}\par
00172 $requiereVerificacion = !empty($_POST[{\cf22 'contrasena'}]);\par
00173 \par
00174 {\cf19 if} ($requiereVerificacion) \{\par
00175     {\cf20 // 3A. CONTRASE\'D1AS COINCIDENTES}\par
00176     {\cf19 if} ($_POST[{\cf22 'contrasena'}] !== $_POST[{\cf22 'repetir-contrasena'}]) \{\par
00177         $_SESSION[{\cf22 'mensaje_error'}] = {\cf22 "Las contrase\'F1as nuevas no coinciden."};\par
00178         header({\cf22 "Location: $mensaje_destino"});\par
00179         exit;\par
00180     \}\par
00181     {\cf20 // ------------------------------------}\par
00182 \par
00183     {\cf20 // 3B. CONTRASE\'D1AS CON M\'C1S DE 8 CARACTERES}\par
00184     {\cf19 if}(strlen($_POST[{\cf22 'contrasena'}]) < 8) \{\par
00185         $_SESSION[{\cf22 'mensaje_error'}] = {\cf22 "La contrase\'F1a debe tener al menos 8 caracteres."};\par
00186         header({\cf22 "Location: $mensaje_destino"});\par
00187         exit;\par
00188     \}\par
00189     {\cf20 // ------------------------------------}\par
00190 \par
00191     {\cf20 // 3C. CONTRASE\'D1A CON AL MENOS UN NUMERO, UNA MAY\'DASCULA Y UN CARACTER ESPECIAL}\par
00192     {\cf20 // Contrase\'F1a Nueva del POST}\par
00193     $passwordNueva = $_POST[{\cf22 'contrasena'}];\par
00194 \par
00195     {\cf20 // 1. Verificar si contiene al menos un n\'FAmero (/\\d/)}\par
00196     $tieneNum = preg_match({\cf22 '/\\d/'}, $passwordNueva);\par
00197 \par
00198     {\cf20 // 2. Verificar si contiene al menos una may\'FAscula (/[A-Z]/)}\par
00199     $tieneMay = preg_match({\cf22 '/[A-Z]/'}, $passwordNueva);\par
00200 \par
00201     {\cf20 // 3. Verificar si contiene al menos un car\'E1cter especial}\par
00202     {\cf20 // Nota: Solo se incluyen los caracteres que has definido en tu JS.}\par
00203     $tieneEsp = preg_match({\cf22 '/[!@#$%^&*(),.?":\{\}|<>]/'}, $passwordNueva);\par
00204 \par
00205     {\cf20 // Si NO tiene n\'FAmero O NO tiene may\'FAscula O NO tiene especial, fallar.}\par
00206     {\cf19 if} (!$tieneNum || !$tieneMay || !$tieneEsp) \{\par
00207         $_SESSION[{\cf22 'mensaje_error'}] = {\cf22 "La contrase\'F1a debe contener al menos un n\'FAmero, una may\'FAscula y un car\'E1cter especial."};\par
00208         header({\cf22 "Location: $mensaje_destino"});\par
00209         exit;\par
00210     \}\par
00211     {\cf20 // ------------------------------------}\par
00212 \par
00213     {\cf20 // 3B. CAMPO DE CONTRASE\'D1A ANTIGUO VAC\'CDO}\par
00214     {\cf19 if} (empty($passwordAntigua)) \{\par
00215         $_SESSION[{\cf22 'mensaje_error'}] = {\cf22 "Debe introducir la contrase\'F1a antigua para establecer una nueva."};\par
00216         header({\cf22 "Location: $mensaje_destino"});\par
00217         exit;\par
00218     \}\par
00219     {\cf20 // ------------------------------------}\par
00220 \par
00221     {\cf20 // Obtener el hash almacenado}\par
00222     $stmt = $conn->prepare({\cf22 "SELECT password FROM usuario WHERE id = ?"});\par
00223     $stmt->bind_param({\cf22 "i"}, $id);\par
00224     $stmt->execute();\par
00225     $res = $stmt->get_result();\par
00226 \par
00227     {\cf20 // 3C. ERROR DE SEGURIDAD. El ID existe en la sesi\'F3n, pero no en la DB.}\par
00228     {\cf19 if} ($res->num_rows === 0) \{\par
00229         {\cf20 // Usuario logeado no encontrado}\par
00230         $_SESSION[{\cf22 'mensaje_error'}] = {\cf22 "Error de autenticaci\'F3n interna."};\par
00231         header({\cf22 "Location: $mensaje_destino"});\par
00232         exit;\par
00233     \}\par
00234 \par
00235     {\cf20 // Guardar la contrase\'F1a del usaurio en $hashAlmacenado}\par
00236     $user = $res->fetch_assoc();\par
00237     $hashAlmacenado = $user[{\cf22 'password'}];\par
00238     $stmt->close();\par
00239 \par
00240     {\cf20 // Verificar la contrase\'F1a antigua}\par
00241     {\cf19 if} (!password_verify($passwordAntigua, $hashAlmacenado)) \{\par
00242         $_SESSION[{\cf22 'mensaje_error'}] = {\cf22 "La contrase\'F1a antigua es incorrecta. Por favor, int\'E9ntelo de nuevo."};\par
00243         header({\cf22 "Location: $mensaje_destino"});\par
00244         exit;\par
00245     \}\par
00246 \par
00247     {\cf20 // Hasheamos la contrase\'F1a nueva antes de enviarla a la l\'F3gica de negocio.}\par
00248     $data[{\cf22 'password'}] = password_hash($_POST[{\cf22 'contrasena'}], PASSWORD_DEFAULT);\par
00249 \par
00250     {\cf20 // Si llegamos aqu\'ED, la contrase\'F1a antigua es correcta.}\par
00251 \}\par
00252 \par
00253 {\cf20 // ----------------------------------------------------------------------------------------}\par
00254 {\cf20 // 4. \'BFHay algo que modificar?}\par
00255 {\cf20 // ----------------------------------------------------------------------------------------}\par
00256 \par
00257 {\cf20 // No hay nada que modificar? --> Error}\par
00258 {\cf19 if} (count($data) === 1) \{\par
00259     $_SESSION[{\cf22 'mensaje_error'}] = {\cf22 "No se detectaron modificaciones para guardar."};\par
00260     header({\cf22 "Location: $mensaje_destino"});\par
00261     exit;\par
00262 \}\par
00263 \par
00264 {\cf20 // ----------------------------------------------------------------------------------------}\par
00265 {\cf20 // 4. Llamar a la l\'F3gica de negocio}\par
00266 {\cf20 // ----------------------------------------------------------------------------------------}\par
00267 \par
00268 {\cf20 // Guardamos el resultado de la llamada a acutalizarUsuario (metodo de la l\'F3gica de negocio)}\par
00269 $resultado = actualizarUsuario($conn, $data);\par
00270 \par
00271 {\cf20 // Actualizar variables de sesi\'F3n si todo sali\'F3 bien}\par
00272 {\cf19 if} ($resultado[{\cf22 "status"}] === {\cf22 "ok"}) \{\par
00273 \par
00274     {\cf19 if} (isset($data[{\cf22 'nombre'}])) \{\par
00275         $_SESSION[{\cf22 'usuario_nombre'}] = $data[{\cf22 'nombre'}];\par
00276     \}\par
00277     {\cf19 if} (isset($data[{\cf22 'apellidos'}])) \{\par
00278         $_SESSION[{\cf22 'usuario_apellidos'}] = $data[{\cf22 'apellidos'}];\par
00279     \}\par
00280     {\cf19 if} (isset($data[{\cf22 'gmail'}])) \{\par
00281         $_SESSION[{\cf22 'usuario_correo'}] = $data[{\cf22 'gmail'}];\par
00282     \}\par
00283     {\cf20 // Mostrar mensajes de error o de \'E9xito}\par
00284     $_SESSION[{\cf22 'mensaje_exito'}] = {\cf22 "Perfil actualizado correctamente."};\par
00285 \}\par
00286 \par
00287 {\cf20 // Redirigir de vuelta al perfil}\par
00288 header({\cf22 "Location: ../html/perfil_cliente.php"});\par
00289 exit;\par
00290 \par
00291 {\cf20 // ----------------------------------------------------------------------------------------}\par
00292 {\cf20 // ----------------------------------------------------------------------------------------}\par
00293 {\cf20 // ----------------------------------------------------------------------------------------}\par
00294 {\cf20 // ----------------------------------------------------------------------------------------}\par
}
