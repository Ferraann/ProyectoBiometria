01320         {\cf19 if} ($this->Timelimit > 0) \{\par
01321             $endtime = time() + $this->Timelimit;\par
01322         \}\par
01323         $selR = [$this->smtp_conn];\par
01324         $selW = {\cf17 null};\par
01325         {\cf19 while} (is_resource($this->smtp_conn) && !feof($this->smtp_conn)) \{\par
01326             {\cf20 //Must pass vars in here as params are by reference}\par
01327             {\cf20 //solution for signals inspired by https://github.com/symfony/symfony/pull/6540}\par
01328             set_error_handler({\cf17 function} () \{\par
01329                 call_user_func_array([$this, {\cf22 'errorHandler'}], func_get_args());\par
01330             \});\par
01331             $n = stream_select($selR, $selW, $selW, $this->Timelimit);\par
